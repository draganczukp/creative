{
	"name": "p5-ts-starter",
	"version": "1.0.0",
	"description": "p5 typescript starter",
	"license": "CC-BY",
	"repository": {
		"url": "https://github.com/draganczukp/p5-ts-starter",
		"type": "github"
	},
	"scripts": {
		"dev": "npm-run-all --parallel live watch-sass watch-ts",
		"live": "live-server --wait=600",
		"watch-sass": "sass --watch src/scss:dist/css",
		"watch-ts": "tsc -w",
		"build": "sass src/scss:dist/css && tsc"
	},
	"devDependencies": {
		"@types/p5": "^0.7.2",
		"live-server": "^1.2.0",
		"npm-run-all": "^4.1.3",
		"sass": "^1.15.1",
		"typescript": "^3.1.6"
	}
}
